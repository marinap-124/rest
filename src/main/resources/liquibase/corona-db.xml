<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet author="marina" id="corona">
		<createTable tableName="corona">
			<column autoIncrement="true" name="id" type="INT">
				<constraints primaryKey="true"/>
			</column>
			<column name="area_code" remarks="area_code" type="VARCHAR(10)"/>
			<column name="count" remarks="count" type="INT"/>
			<column name="week_name" remarks="week_name" type="VARCHAR(300)"/>
		</createTable>
	</changeSet>

</databaseChangeLog>
